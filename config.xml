<?xml version='1.0' encoding='utf-8'?>
<widget id="io.kitecs.ais" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KiteCS AIS</name>
    <description>Starter project with Angular and Ionic</description>
    <author email="hello@kemalyalcinkaya.co.uk" href="http://kemalyalcinkaya.co.uk">Kemal Yalcinkaya</author>
    <content src="index.html"/>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-device" spec="1.1.5"/> 
    <plugin name="cordova-plugin-statusbar" spec="2.2.1"/>       
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-background-mode" spec="0.7.2"/>    
</widget>
