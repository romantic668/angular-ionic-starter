    "string-replace-loader": "~1.0.5",
    "style-loader": "^0.13.2",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "~1.1.1",
    "ts-loader": "^2.0.1",
    "ts-node": "~2.1.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "2.2.1",
    "watch-run": "~1.2.4",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "^2.1.1",
