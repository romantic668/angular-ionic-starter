{
  "name": "angular-ionic-starter",
  "version": "0.0.1",
  "description": "Kite CS : Angular Ionic Starter",
  "author": "Kemal Yalcinkaya (KiteCS)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitecs/angular-ionic-starter.git"
  },
  "scripts": {
    "postinstall": "node hooks/postinstall",
    "/": "------------------------DEV--------------------------",
    "dev:build:dll": "npm run clean:dll && webpack",
    "dev:start": "npm run webdev",
    "dev:start:hmr": "npm run webdev:hmr",
    "//": "-----------------------PROD-------------------------",
    "prod:compile": "npm run clean:compile && npm run ngc",
    "prod:build:web": "npm run clean:dist && webpack",
    "prod:build:cordova": "npm run clean:cordova && webpack",
    "prod:build:ios": "cd cordova && cordova build ios && cd ..",
    "prod:build:android": "cd cordova && cordova build android && cd ..",
    "prod:start": "node src/server/server.prod.js",
    "///": "-------------------TEST/E2E/CI---------------------",
    "pretest": "npm run lint",
    "pretest:once": "npm run lint",
    "pretest:once:ci": "npm run lint",
    "test:once": "karma start",
    "test:once:ci": "karma start",
    "test": "karma start",
    "coverage": "coveralls < ./coverage/lcov.info",
    "ci": "npm run e2e:jit && npm run test:once:ci",
    "ci:testall": "npm run e2e:testall && npm run test:once:ci",
    "tslint": "node node_modules/tslint/bin/tslint",
    "lint": "npm run tslint \"src/app/**/*.ts\" ",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "protractor": "protractor",
    "pree2e:only": "npm run webdriver:update -- --standalone",
    "e2e": "npm-run-all -p -r e2e:server e2e:only",
    "e2e:testall": "npm run e2e:jit && npm run e2e:aot",
    "e2e:aot": "npm run compile:e2e && npm run e2e",
    "e2e:jit": "npm run build:prod:e2e && npm run e2e",
    "e2e:universal": "npm run build:universal:prod:e2e && npm-run-all -p -r server:universal e2e:only:universal",
    "e2e:universal:aot": "npm run compile:universal:e2e && npm-run-all -p -r server:universal e2e:only:universal",
    "e2e:only:universal": "npm run protractor -- --universal",
    "e2e:only": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e:server": "node src/server/server.prod",
    "////": "---------------------UTILS------------------------",
    "webdev": "webpack-dev-server",
    "webdev:hmr": "npm run webdev -- --inline --hot",
    "webpack": "webpack",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json",
    "ngc:universal": "./node_modules/.bin/ngc -p tsconfig.aot.universal.json",
    "clean": "npm run clean:compile && npm run rimraf -- doc typings cordova coverage dist .awcache dll",
    "clean:dist": "npm run rimraf -- dist .awcache",
    "clean:dll": "npm run rimraf -- dll",
    "clean:compile": "npm run rimraf -- \"compiled\" ",
    "clean:coverage": "npm run rimraf -- \"coverage\" ",
    "clean:cordova": "npm run rimraf -- cordova/www",
    "compile:aot": "npm run clean:compile && npm run ngc && npm run build:aot",
    "compile:e2e": "npm run clean:compile && npm run ngc && npm run build:aot:prod:e2e",
    "compile:universal": "npm run clean:compile && npm run ngc:universal && npm run build:universal:aot",
    "compile:universal:e2e": "npm run clean:compile && npm run ngc:universal && npm run build:universal:aot:prod:e2e",
    "compile:dev": "npm run clean:compile && npm run ngc && npm run build:aot:dev",
    "compile:only": "npm run clean:compile && npm run ngc",
    "compile:watch": "watch-run -i npm run compile:only -p 'src/app/**/*.ts, src/app/**/*.scss' npm run compile:only",
    "start:abbas": "node src/server/server.prod",
    "start:old": "npm run server:dev",
    "start:hmr": "npm run server:hmr",
    "server:dev": "npm run webdev",
    "server:hmr": "npm run webdev:hmr",
    "server:prod": "npm-run-all -p -r watch:prod prodserver",
    "server:prod80": "npm-run-all -p -r serve80 watch:prod",
    "server:universal": "nodemon --watch dist --exec node dist/server/index.js",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "build": "npm run build:prod",
    "build:cordova": "npm run clean:cordova && webpack",
    "build:cordova:android": "cd cordova && cordova build android && cd .. && npm run prodserver",
    "build:aot": "npm run build:aot:prod",
    "build:aot:prod": "npm run clean:dist && webpack",
    "build:aot:prod:e2e": "npm run clean:dist && webpack",
    "build:aot:dev": "npm run clean:dist && webpack",
    "build:dev": "npm run clean:dist && webpack",
    "build:dll": "npm run clean:dll && webpack",
    "build:prod": "npm run clean:dist && webpack",
    "build:prod:e2e": "npm run clean:dist && webpack",
    "build:universal": "npm run build:universal:prod",
    "build:universal:aot": "npm run build:universal:aot:prod",
    "build:universal:aot:dev": "npm run clean:dist && webpack",
    "build:universal:aot:prod": "npm run clean:dist && webpack",
    "build:universal:aot:prod:e2e": "npm run clean:dist && webpack",
    "build:universal:dev": "npm run clean:dist && webpack",
    "build:universal:prod": "npm run clean:dist && webpack",
    "build:universal:prod:e2e": "npm run clean:dist && webpack",
    "watch": "npm run watch:dev",
    "watch:dev": "npm-run-all -p -r \"build:dev -- --watch\" ",
    "watch:prod": "npm-run-all -p -r \"build:prod -- --watch\" ",
    "watch:universal": "npm-run-all -p -r \"build:universal -- --watch\" ",
    "universal": "npm run build:universal && npm run server:universal",
    "universal:aot": "npm run compile:universal && npm run server:universal",
    "universal:watch": "npm run build:universal && npm-run-all -p -r watch:universal server:universal",
    "rimraf": "rimraf"
  },
  "dependencies": {
    "@angular/common": "4.0.0-rc.1",
    "@angular/compiler": "4.0.0-rc.1",
    "@angular/compiler-cli": "4.0.0-rc.1",
    "@angular/core": "4.0.0-rc.1",
    "@angular/forms": "4.0.0-rc.1",
    "@angular/http": "4.0.0-rc.1",
    "@angular/platform-browser": "4.0.0-rc.1",
    "@angular/platform-browser-dynamic": "4.0.0-rc.1",
    "@angular/platform-server": "4.0.0-rc.1",
    "@angular/router": "4.0.0-rc.1",
    "@angular/tsc-wrapped": "4.0.0-rc.1",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@angularclass/idle-preload": "^1.0.4",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/router-store": "1.2.5",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.3",
    "@ngrx/store-log-monitor": "3.0.2",
    "angular2-express-engine": "2.1.0-rc.1",
    "angular2-platform-node": "2.1.0-rc.1",
    "angular2-universal": "2.1.0-rc.1",
    "angular2-universal-polyfills": "2.1.0-rc.1",
    "body-parser": "1.16.1",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "express": "4.14.1",
    "ionic-angular": "^2.1.0",
    "ionicons": "^3.0.0",
    "methods": "1.1.2",
    "ngrx-store-freeze": "0.1.9",
    "ngrx-store-logger": "0.1.7",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@types/body-parser": "~0.0.33",
    "@types/compression": "~0.0.33",
    "@types/cookie-parser": "~1.3.30",
    "@types/core-js": "~0.9.35",
    "@types/express": "~4.0.33",
    "@types/express-serve-static-core": "~4.0.37",
    "@types/ip": "~0.0.29",
    "@types/jasmine": "2.5.43",
    "@types/mime": "~0.0.29",
    "@types/node": "~7.0.5",
    "@types/selenium-webdriver": "~2.53.40",
    "@types/serve-static": "~1.7.31",
    "angular-router-loader": "~0.5.0",
    "angular2-template-loader": "~0.6.0",
    "autoprefixer": "^6.7.6",
    "awesome-typescript-loader": "~3.0.0-beta.17",
    "codelyzer": "^2.0.1",
    "compression-webpack-plugin": "~0.3.1",
    "copy-webpack-plugin": "~4.0.0",
    "cordova": "^6.5.0",
    "coveralls": "^2.11.16",
    "css-loader": "~0.26.2",
    "es6-promise": "~4.0.5",
    "es6-promise-loader": "~1.0.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "fs-extra": "^2.0.0",
    "fs-promise": "^2.0.0",
    "html-webpack-plugin": "~2.28.0",
