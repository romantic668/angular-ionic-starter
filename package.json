    "webpack-merge": "~3.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/common",
      "@angular/compiler",
      "@angular/compiler-cli",
      "@angular/core",
      "@angular/forms",
      "@angular/http",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/platform-server",
      "@angular/router",
      "@angular/tsc-wrapped",
      "protractor"
    ]
  },
  "engines": {
    "node": ">= 7.4.0",
