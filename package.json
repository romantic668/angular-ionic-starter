{
  "name": "angular-ionic-starter",
  "version": "0.0.2",
  "description": "Kite CS : Angular Ionic Starter",
  "author": "Kemal Yalcinkaya (KiteCS)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitecs/angular-ionic-starter.git"
  },
  "scripts": {
    "postinstall": "node hooks/postinstall",
    "clean": "npm run clean:compile && npm run utils:rimraf -- doc typings cordova coverage dist .awcache dll",
    "clean:dist": "npm run utils:rimraf -- dist .awcache",
    "clean:dll": "npm run utils:rimraf -- dll",
    "clean:compile": "npm run utils:rimraf -- \"compiled\" ",
    "clean:coverage": "npm run utils:rimraf -- \"coverage\" ",
    "clean:cordova": "npm run utils:rimraf -- cordova/www",
    "dev:build:dll": "npm run clean:dll && webpack",
    "dev:start": "npm run utils:webdev",
    "dev:start:hmr": "npm run utils:webdev:hmr",
    "prod:compile": "npm run clean:compile && npm run utils:ngc",
    "prod:build:web": "npm run clean:dist && webpack",
    "prod:build:cordova": "npm run clean:cordova && webpack",
    "prod:build:ios": "cd cordova && cordova build ios && cd ..",
    "prod:build:android": "cd cordova && cordova build android && cd ..",
    "prod:start": "node src/server/server.prod.js",
    "lint": "npm-run-all lint:*",
    "lint:ts": "node node_modules/tslint/bin/tslint \"src/app/**/*.ts\" ",
    "lint:js": "./node_modules/.bin/eslint \"src/app/**/*.js\" ",
    "pretest": "npm run lint",
    "test": "karma start",
    "test:once": "karma start",
    "test:coverage": "coveralls < ./coverage/lcov.info",
    "test:ci": "npm run e2e:aot && npm run test:once",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "protractor": "protractor",
    "pree2e:only": "npm run webdriver:update -- --standalone",
    "e2e": "npm-run-all -p -r e2e:server e2e:only",
    "e2e:testall": "npm run e2e:jit && npm run e2e:aot",
    "e2e:aot": "npm run clean:compile && npm run utils:ngc && npm run build:aot:prod:e2e && npm run e2e",
    "e2e:jit": "npm run build:prod:e2e && npm run e2e",
    "e2e:only": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e:server": "node src/server/server.prod",
    "utils:rimraf": "rimraf",
    "utils:webdev": "webpack-dev-server",
    "utils:webdev:hmr": "npm run utils:webdev -- --inline --hot",
    "utils:webpack": "webpack",
    "utils:ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json"    
  },
